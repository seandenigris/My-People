Class {
	#name : #MpOrganization,
	#superclass : #MpRelatableEntity,
	#traits : 'MpTAlphabetizable',
	#classTraits : 'MpTAlphabetizable classTrait',
	#instVars : [
		'children',
		'parent'
	],
	#category : #'MyPeople-Core'
}

{ #category : #accessing }
MpOrganization class >> exampleLeakyCauldron [
	<gtExample>
	^ self 
		given: [ self named: 'Leaky Cauldron' ]
		when: [ :org |
			org addresses add: 'Daigon Alley'.
			org ]
		then: [ :org |
			org addresses should haveSize: 1.
			org addresses first should equal: 'Daigon Alley' ]
]

{ #category : #accessing }
MpOrganization class >> exampleOllivanders [
	<gtExample>
	^ self 
		given: [ self named: 'Ollivanders' ]
		when: [ :org |
			org addresses add: 'Daigon Alley'.
			org ]
		then: [ :org |
			org addresses should haveSize: 1.
			org addresses first should equal: 'Daigon Alley' ]
]

{ #category : #accessing }
MpOrganization class >> examplePhillyZoo [
	<gtExample>
	^ self named: 'Philadelphia Zoo'
]

{ #category : #accessing }
MpOrganization class >> exampleSimilarity [
	<gtExample>
	^ self examplePhillyZoo similarityByNameTo: (self named: 'Philly Zoo')
]

{ #category : #accessing }
MpOrganization class >> named: aString [
	^ self new
		name: aString;
		yourself
]

{ #category : #accessing }
MpOrganization >> addChild: anOrganization [
	
	self validateSameAddressBookAs: anOrganization.
	anOrganization basicParent: self.
	^ self children add: anOrganization "Children currently a set so don't worry about duplicate adding"
]

{ #category : #accessing }
MpOrganization >> addChildNamed: aString [
	| child |
	child := (self class named: aString)
		parent: self;
		yourself.
	^ self addChild: child
]

{ #category : #'as yet unclassified' }
MpOrganization >> affiliations: aCollection [ 
	
	| toRemove toAdd |
	toRemove := self affiliations difference: aCollection.
	toRemove do: #unlink.
	
	toAdd := aCollection difference: self affiliations.
	toAdd do: #link
]

{ #category : #accessing }
MpOrganization >> basicParent: anOrganization [
	parent ifNotNil: [ parent children remove: self ].
	parent := anOrganization.
]

{ #category : #accessing }
MpOrganization >> children [
	^ self maLazyInstVarUsing: self childrenDescription
]

{ #category : #accessing }
MpOrganization >> children: anObject [
	children := anObject
]

{ #category : #'magritte-descriptions' }
MpOrganization >> childrenDescription [
	<magritteDescription>
	
	^ MAToManyRelationDescription new
		accessor: #children;
		priority: 500;
		default: Set new;
		classes: MpOrganization withAllSubclasses;
		comment: 'Not too fleshed out - subsidiaries? Locations?';
		gtSearchSource: (MessageSend receiver: MpAddressBook selector: #uniqueInstance);
		yourself
]

{ #category : #accessing }
MpOrganization >> childrenGtSearchExistingFor: aSearch [
	<gtSearch>
	
	^ aSearch list
		title: 'Add Child (existing)';
		priority: 50;
		items: [ self addressBook organizations privateContacts ];
		filterBySubstring;
		send: [ :anObject | self addChild: anObject ]
]

{ #category : #accessing }
MpOrganization >> childrenGtSpotNewFor: aSearch [
	<gtSearch>
	
	^ aSearch explicit
		title: 'New Child';
		priority: 50;
		itemName: [ :result | '+ ', result gtDisplayText ];
		search: [ :aContext | 
			| input |
			input := aContext searchQuery trimBoth.
			{ MpOrganization new 
				name: input; 
				yourself } asAsyncStream ];
		send: [ :anObject | self addChild: anObject ]
]

{ #category : #defaults }
MpOrganization >> defaultAvatar [
	^ Form fromBinaryStream: self defaultAvatarBase64Encoded base64Decoded readStream binary
]

{ #category : #defaults }
MpOrganization >> defaultAvatarBase64Encoded [
	"Store Building Flat Icon Vector.svg from Wikimedia Commons by Videoplasty.com, CC-BY-SA 4.0"
	^ ''
]

{ #category : #accessing }
MpOrganization >> fullName [
	^ String streamContents: [ :s |
			self parent ifNotNil: [ :p | s << p fullName; space ].
			s << self name ]
]

{ #category : #accessing }
MpOrganization >> gtViewChildrenFor: aView [
	<gtView>

	self children isEmptyOrNil ifTrue: [ ^ aView empty ].
	
	^ aView list
		title: 'Children' translated;
		priority: 60;
		items: [ self children asOrderedCollection ];
		actionUpdateButtonTooltip: 'Update'.
]

{ #category : #testing }
MpOrganization >> isOrganization [
	^ true
]

{ #category : #testing }
MpOrganization >> isPerson [
	^ false
]

{ #category : #'magritte-descriptions' }
MpOrganization >> nameDescription [
	<magritteDescription>
	^ MAStringDescription new
			accessor: #name;
			label: 'Name';
			priority: 100;
			yourself
]

{ #category : #accessing }
MpOrganization >> parent [
	^ self maLazyInstVarUsing: self parentDescription
]

{ #category : #accessing }
MpOrganization >> parent: anOrganization [

	anOrganization ifNil: [
		parent := nil.
		^ self ].
	
	self validateSameAddressBookAs: anOrganization.
	
	self basicParent: anOrganization.
	anOrganization addChild: self.
]

{ #category : #'magritte-descriptions' }
MpOrganization >> parentDescription [
	<magritteDescription>
	^ MAToOneRelationDescription new
			accessor: #parent;
			priority: 500;
			classes: { MpOrganization };
			comment: 'Not too fleshed out - subsidiaries? Locations?';
			gtSearchSource: (MessageSend receiver: MpAddressBook selector: #uniqueInstance);
			yourself
]

{ #category : #printing }
MpOrganization >> printOn: aStream [
	aStream << self fullName
]

{ #category : #accessing }
MpOrganization >> similarityByNameTo: rhs [
	| myWords theirWords wordScores |
	myWords := self name mpFullNameWords.
	theirWords := rhs name mpFullNameWords.
	wordScores := myWords collectWithIndex: [ :w :i |
		| theirWord |
		theirWord := theirWords at: i.
		w = theirWord 
			ifTrue: [ 100 percent ]
			ifFalse: [ 0.0 ] ].
	^ wordScores average.
	
]

{ #category : #accessing }
MpOrganization >> validateSameAddressBookAs: anOrganization [ 
	| isConflict |
	"Address book cannot be nil - see #addressBook:"
	
	"If both non-nil equal, no action needed"
	anOrganization addressBook = self addressBook ifTrue: [ ^ self ].
	
	"If we get here, the libraries are not equal"
	isConflict := anOrganization addressBook isNotNil and: [ self addressBook  isNotNil ].
	isConflict ifTrue: [ ^ self error: 'Address books conflict' ].
]
